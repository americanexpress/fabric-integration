{
  "name": "fabric-integration",
  "version": "1.0.0",
  "description": "high-level node sdk",
  "main": "dist/Gateway.js",
  "types": "dist/Gateway.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "./node_modules/tslint/bin/tslint 'src/app/**/*.ts?(x)'",
    "build": "./node_modules/typescript/bin/tsc",
    "prepare":"yarn build",
    "test": "XUNIT_FILE=src/test/xunit.xml nyc mocha src/test/unit/*.spec.ts src/test/unit/**/*.spec.ts --require ts-node/register -R spec-xunit-file",
    "test:integration": "mocha src/test/integration/e2e.spec.ts --require ts-node/register",
    "docs": "typedoc --out docs --mode modules"
  },
  "engines": {
    "node": "^8.9.0 || ^10.15.3 || ^12.13.1",
    "npm": "^5.5.1 || ^6.4.1"
  },
  "keywords": [
    "hyperledger",
    "fabric",
    "node",
    "client"
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.12.2",
    "@types/sinon": "^7.0.11",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.9",
    "nyc": "^15.0.0",
    "sinon": "^7.2.3",
    "spec-xunit-file": "^0.0.1-3",
    "ts-node": "^8.0.3",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "typedoc": "^0.15.6",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@types/decompress": "^4.2.3",
    "@types/fs-extra": "^5.0.5",
    "decompress": "^4.2.0",
    "fabric-ca-client": "1.4.5",
    "fabric-client": "1.4.5",
    "fs-extra": "^7.0.1",
    "log4js": "^4.0.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "replace": "^1.0.1",
    "util": "^0.11.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text",
      "cobertura",
      "html",
      "lcov"
    ],
    "exclude": [
      "docs",
      "coverage",
      "src/test",
      "config",
      "dist",
      "types",
      "src/app/types.ts"
    ],
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "check-coverage": true
  }
}
