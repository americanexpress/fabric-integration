{
  "extends": ["tslint:latest", "tslint-config-airbnb"],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-name": [false, "always-prefix"],
    "prefer-array-literal": [true, { "allow-type-parameters": true }],
    "import-name": [
      true,
      {
        "fabricClient": "FabricClient"
      }
    ]
  }
}
